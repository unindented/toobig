{
  "projects": [
    "packages/project-a",
    "packages/project-b"
  ],
  "reporters": [
    "default",
    [
      "table",
      {
        "template": "markdown",
        "color": false,
        "maxPathLength": 36,
        "verbose": false,
        "output": "toobig/report.md"
      }
    ],
    [
      "json",
      {
        "output": "toobig/report.json"
      }
    ],
    [
      "junit",
      {
        "output": "toobig/report.xml"
      }
    ],
    [
      "tap",
      {
        "output": "toobig/report.txt"
      }
    ]
  ]
}
